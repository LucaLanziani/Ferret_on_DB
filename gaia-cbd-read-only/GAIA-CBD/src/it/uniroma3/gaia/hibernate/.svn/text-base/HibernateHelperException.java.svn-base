package it.uniroma3.gaia.hibernate;

public class HibernateHelperException extends Exception {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	public HibernateHelperException() {
		super();
	}

	public HibernateHelperException(String message, Throwable cause) {
		super(message, cause);
	}

	public HibernateHelperException(String message) {
		super(message);
	}

	public HibernateHelperException(Throwable cause) {
		super(cause);
	}
}
